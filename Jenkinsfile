pipeline {
  agent any
  stages {
    stage ("Maven build") {
      steps {
        echo "Hello world welcome to jenkinsfile"
      }
    }
    stage ("main deploy") {
      steps {
        echo "deploying to main"
      }
    }
    stage ("dev deploy") {
      steps {
        echo "deploying to dev"
      }
    }
    stage ("feat deploy") {
      steps {
        echo "deploying to feat"
      }
    }
  }
}
